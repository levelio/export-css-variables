{
  "name": "@levelii/export-css-variables",
  "type": "module",
  "version": "0.0.4",
  "private": false,
  "packageManager": "pnpm@9.12.3",
  "description": "export css variables to css file",
  "author": "levelio",
  "license": "MIT",
  "homepage": "https://github.com/levelio/export-css-variables#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/levelio/export-css-variables.git"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "ecv": "bin/ecv.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "bin": "esno src/commands/ecv.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "postcss": "^8.4.47",
    "postcss-less": "^6.0.0",
    "postcss-value-parser": "^4.2.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@types/fs-extra": "^11.0.4",
    "@types/postcss-less": "^4.0.6",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "esno": "^4.8.0",
    "pnpm": "9.12.0",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.4"
  }
}
